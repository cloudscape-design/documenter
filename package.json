{
  "name": "@cloudscape-design/documenter",
  "version": "1.0.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/cloudscape-design/documenter.git"
  },
  "homepage": "https://cloudscape.design",
  "main": "./dist/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "lint": "eslint --ignore-path .gitignore --ext js,ts,tsx .",
    "build": "tsc",
    "test": "jest",
    "docs-playground": "cd docs-playground && vite",
    "gen-components-docs": "npm run build-plugins && typedoc --options typedoc-components.js",
    "gen-docs": "npm run build-plugins && typedoc --options typedoc-components.js && typedoc --options typedoc-dev-toolkit.js && typedoc --options typedoc-test-utils.js",
    "build-plugins": "npx tsc --project typedoc-plugin/tsconfig.json && npx tsc --project typedoc-plugin-components/tsconfig.json && npx tsc --project typedoc-plugin-dev-toolkit/tsconfig.json && npx tsc --project typedoc-plugin-test-utils/tsconfig.json",
    "prepublishOnly": "npm run build",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@cloudscape-design/components": "^3.0.0",
    "@cloudscape-design/global-styles": "^1.0.0",
    "@cloudscape-design/test-utils-core": "^1.0.0",
    "@tsconfig/node12": "^1.0.9",
    "@types/jest": "^26.0.23",
    "@types/micromatch": "^4.0.1",
    "@types/node": "^17.0.36",
    "@types/react-dom": "^16.9.8",
    "@types/react": "^16.14.8",
    "@typescript-eslint/eslint-plugin": "^4.28.5",
    "@typescript-eslint/parser": "^4.28.5",
    "@vitejs/plugin-react": "^1.3.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-header": "^3.1.1",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-unicorn": "^34.0.1",
    "eslint": "^7.31.0",
    "husky": "^7.0.0",
    "jest": "^27.0.3",
    "lint-staged": "^11.1.1",
    "prettier": "^2.3.2",
    "react-dom": "^16.14.0",
    "react-router-dom": "^6.3.0",
    "react-router": "^6.3.0",
    "react": "^16.14.0",
    "remarkable-react": "^1.4.3",
    "remarkable": "^2.0.1",
    "ts-jest": "^27.0.1",
    "ts-node": "^10.8.0",
    "tslib": "^2.4.0",
    "typescript": "^4.6.3",
    "vite": "^2.9.5"
  },
  "dependencies": {
    "change-case": "^4.1.1",
    "micromatch": "^4.0.2",
    "typedoc": "~0.19.2",
    "typedoc22": "npm:typedoc@~0.22.15"
  },
  "lint-staged": {
    "*.{ts,tsx,js}": [
      "eslint --fix"
    ]
  }
}
